<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Franklinsqlife.Login">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Venta de Login!"
                   />
            <Entry x:Name="txtUsuario" Placeholder="Ingrese Usuario"/>
            <Entry x:Name="txtContraseña" Placeholder="Ingrese contrasena"/>
            <Button x:Name="btnIniciar" Text="Iniciar Sesion" Clicked="btnInciar_Iniciar"/>
            <Button x:Name="btnRegistrar" Text=" Registar" Clicked=" inicie sesion"/>

        </StackLayout>
    </ContentPage.Content>
</ContentPage>